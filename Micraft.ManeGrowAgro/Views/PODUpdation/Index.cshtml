    function DownloadFile(Count) {
        window.location = '/PODUpdation/Download?file=' + $("#FileName" + Count).val();
    }
  
    function DeleteFile(Count) {
        var x = confirm("Are you sure you want to delete?");
        if (x) {
            $.ajax({
                type: "GET",
                url: '/PODUpdation/DeleteFiles',
                data: { 'ID': $('#ID' + Count).val(), FileName: $('#FileName' + Count).val(), Mode: "Edit" },
                success: function (result) {
                    if (result == "success") {
                        toastr.success("File Deleted Successfully..!");
                        $("#R" + Count + "").remove();
                    }
                    else {
                        toastr.error(result);
                    }
                }
            })
        }
        else {
            return false;
        }
    }

    function RejectionModel(ID) {       
        $('#HFID').val(ID);
        $.ajax({
            type: "GET",
            url: '/PODUpdation/GetRejectionHistory',
            data: { 'ID': ID },
            success: function (result) {
                if (result.Message == "success") {
                    debugger

                    if (result.PODDetails.RejectedQty != 0) {
                        $('#RejectionModal').modal({ backdrop: 'static', keyboard: false })
                        $("#RejectionModal").modal("show");
                        $('#RejectedQty').val(result.PODDetails.RejectedQty);
                        $('#OrderNos').val(result.PODDetails.OrderNo);

                        $('#CustomerNames').val(result.PODDetails.CustomerName);
                        $('#ProductName').val(result.PODDetails.ProductName);
                        $('#CustID').val(result.PODDetails.CustID);

                        $('#real tr td').parents('tr').remove();
                        //  $("#real").empty();
                        $.each(result.History, function (i, item) {
                            $("#real").append("<tr> <td>" + item.ResaleType + "</td> <td>" + item.SoldCustomerName + "</td> <td>" + item.Rate + "</td><td>" + item.Qty + "</td><td>" + item.TotalAmount + "</td><td> <button type='button' id='edt' class='btn btn-sm btn-primary btnEdit' onclick='Edit(this.id);'><i class='fa fa-edit'></i></button> <Button type='Button' class='btn btn-sm btn-danger btnDelete' href='#'  id='dlt' > <i class='fa fa-trash-alt'></i> </Button></td>   </tr>");

                            $("#Amount").val(0);

                            $("#btnSavve").attr("disabled", true);
                        });
                        alculatewt();
                    } else {
                        alert("you cannot add report because rejection qty is 0")
                    }
                }
                else {
                    toastr.error(result.Message);
                }
            }
        })
    }


    function ValidateResale() {
        $('#CustomerName').attr("disabled", false);
        $('#Rate').attr("disabled", false);
        if ($('#ResaleType').val() != "SOLD") {
            $('#CustomerName').val('');
            $('#Rate').val('');
            $('#Qty').val('');
            $('#Amount').val('');
            $('#CustomerName').attr("disabled", true);
            $('#Rate').attr("disabled", true);
        }

    }
    function calculateTotal() {
        $('#Amount').val(0);
        if ($('#Rate').val() != "" && $('#Qty').val() != "") {
            $('#Amount').val((parseFloat($('#Rate').val()) * parseFloat($('#Qty').val())).toFixed(2));
        }
    }
    function btnadd()
    {
        var valid = true;
        if ($('#ResaleType').val() == null || $('#ResaleType').val() == "") {
            toastr.error("Please Select Resale Type");
            valid = false;
        }

        if ($('#ResaleType').val() == "SOLD") {
            if ($('#CustomerName').val() == null || $('#CustomerName').val() == "") {
                toastr.error("Please Enter Customer Name.");
                valid = false;
            }
            if ($('#Rate').val() == null || $('#Rate').val() == "" || $('#Rate').val() == "0" ) {
                toastr.error("Please Enter Rate.");
                valid = false;
            }
        }


        if ($('#Qty').val() == null || $('#Qty').val() == "" || $('#Qty').val() == "0") {
            toastr.error("Please Enter Qty");
            valid = false;
        }


        if (valid == true) {
            var isexist = false;

            if (isexist == true) {
                toastr.error("Already Added.");
            }
            else {

                $("#real").append("<tr> <td>" + $('#ResaleType').val() + "</td> <td>" + $('#CustomerName').val() + "</td> <td>" + $('#Rate').val() + "</td><td>" + $('#Qty').val() + "</td><td>" + $('#Amount').val() + "</td><td> <button type='button' id='edt' class='btn btn-sm btn-primary btnEdit' onclick='Edit(this.id);'><i class='fa fa-edit'></i></button> <Button type='Button' class='btn btn-sm btn-danger btnDelete' href='#'  id='dlt' > <i class='fa fa-trash-alt'></i> </Button></td>   </tr>");
                $("#CustomerName").val('');
                $("#Rate").val('');
                $("#Qty").val('');
                $("#Amount").val(0);
                alculatewt()
            }



        }
    }
    function alculatewt() {
        $("#lbltot").text(0);
        var Total = 0;
        $("#real TBODY TR").each(function () {
            var row = $(this);
            Total = Total + parseFloat(row.find("TD").eq(3).html());
        });

        $("#lbltot").text(Total);
    }

    function Save() {
        var temp = true;
        if ($('#real TBODY tr').length == 0) {
            toastr.error("Please add Some records.");
            temp = false;
        }


        if (parseFloat($('#lbltot').text()) != parseFloat($('#RejectedQty').val())) {
            toastr.error("Rejected Qty and List Total Qty Must be Same .");
            temp = false;
        }
        if (temp == true) {



            var recs = new Array();
            $("#real TBODY TR").each(function () {
                var row = $(this);
                var Order = {};
                Order.ResaleType = row.find("TD").eq(0).html();
                Order.SoldCustomerName = row.find("TD").eq(1).html();
                Order.Rate = row.find("TD").eq(2).html();
                Order.Qty = row.find("TD").eq(3).html();
                Order.TotalAmount = row.find("TD").eq(4).html();
                recs.push(Order);

            });

            var data = {
                recs: recs, OrderNo: $('#OrderNos').val(), RejectedQty: $("#RejectedQty").val(), ID: $('#HFID').val(), CustID: $("#CustID").val(), CustomerNames: $("#CustomerNames").val(), ProductName: $("#ProductName").val()
            }
            debugger
            $.ajax({
                type: "POST",
                url: "/PODUpdation/SaveData",
                data: JSON.stringify(data),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                    if (result.Message == "success") {

                        $("#real tbody").empty();
                        $("#RejectionModal").modal("hide");
                        toastr.success("Records Saved Succesfully");
                        alculatewt()
                    }
                    else {
                        toastr.error(result.Message);

                    }

                }
            });

        }
    }

    function newexportaction(e, dt, button, config) {
        var self = this;
        var oldStart = dt.settings()[0]._iDisplayStart;
        dt.one('preXhr', function (e, s, data) {
