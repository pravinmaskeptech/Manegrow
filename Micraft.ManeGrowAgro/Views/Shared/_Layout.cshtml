@{
    ManeGrowContext db = new ManeGrowContext();

    HttpContext ctx = HttpContext.Current;
    if (ctx.Session != null)
    {
        //if (ctx.Session.IsNewSession)
        //{
        //    string sessionCookie = ctx.Request.Headers["Cookie"];
        //    if ((null != sessionCookie) && (sessionCookie.IndexOf("ASP.NET_SessionId") >= 0))
        //    {
        //     //   Response.Redirect("~/Account/Login");
        //        return;
        //    }
        //}
    }
    var eName = "";
    var UserName = "";
    var eCode = "";

    var name = "";
    if (User.Identity.Name == "" || User.Identity.Name == null)
    {
        Response.Redirect("~/Account/Login");
    }
    try
    {
        ApplicationUser currentUser = HttpContext.Current.GetOwinContext().GetUserManager<Micraft.ManeGrowAgro.ApplicationUserManager>().FindById(User.Identity.GetUserId());

        if (currentUser == null)
        {
            Response.Redirect("~/Account/Login");
            return;
        }
        eName = currentUser.UserName;
        UserName = currentUser.UserName;

        name = db.EmployeeMasters.Where(x => x.Username == currentUser.UserName).Select(x => x.PhotoUpload).FirstOrDefault();
    }
    catch (Exception EX)
    {
        Response.Redirect("~/Account/Login");
        return;
    }
}
