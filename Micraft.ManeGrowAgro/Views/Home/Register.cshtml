<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <script src="~/plugins/jquery-3.4.1.js"></script>

        <!-- /.content-header -->
        <!-- Main content -->
        <section class="content">
            <div class="col-md-4 col-12 mx-auto pt-md-5 pt-2 pb-3">
                <div class="card" style="background: linear-gradient(170deg, #e2e7db 0%,#a0c76b 100%) !important; ">

                    <div class="card-header text-right">
                        <img src="~/dist/img/logo.png" alt="" class="img-fluid" style="width:100px;">
                    </div>



                    <div class="card-body">

                        <h5><b>Customer Registration</b></h5>
                        <div class="row">
                            <div class="col-12 mb-3">
                                <label>Customer Name</label>
                                <input type="text" id="CustName" required class="form-control form-control-sm" placeholder="Customer Name" data-bv-notempty="true" data-bv-notempty-message="The customer name is required and cannot be empty"></input>
                            </div>
                            <div class="col-6 mb-3">
                                <label>Mobile No</label>
                                <input type="number" id="MobileNumber" required class="form-control form-control-sm" placeholder="10 Digit Mobile No" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>
                            <div class="col-6 mb-3 my-auto pt-3">
                                <button id="sendotp" name="SendOTP" class="btn btn-primary btn-sm">Send OTP</button>
                            </div>
                        </div>
                        <div class="row" id="enterotp">
                            <div class="col-6 mb-3">
                                <label>Enter OTP</label>
                                <input type="number" id="txtotp" max="6" required class="form-control form-control-sm" placeholder="Enter OTP" data-bv-notempty="true" data-bv-notempty-message="The OTP is required and cannot be empty">
                            </div>
                            <div class="col-6 mb-3 my-auto pt-3" id="verifybtn">
                                <button id="verifyotp" name="VerifyOTP" class="btn btn-primary btn-sm">Verify OTP</button>
                            </div>
                        </div>
                        <div class="row" id="otherdetails">
                            <!--/. col -->
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>City</label>
                                <input type="text" id="City" required class="form-control form-control-sm" placeholder="City" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                                @*@Html.DropDownList("City", new SelectList(ViewBag.City, "City", "ID"), new { @class = "form-control form-control-sm" })*@
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>State</label>
                                <input type="text" id="State" required class="form-control form-control-sm" placeholder="State" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>

                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>Area</label>
                                <input type="text" id="Area" required class="form-control form-control-sm" placeholder="Area" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>Pincode</label>
                                <input type="text" id="PinCode" required class="form-control form-control-sm" placeholder="Pincode">
                                @*@Html.DropDownList("PinCode", new SelectList(ViewBag.PinCode, "Pincode", "ID"), new { @class = "form-control form-control-sm" })*@
                            </div>
                            <div class="col-md-12 col-12 mb-3">
                                <label>Address</label>
                                <textarea type="text" id="Address" required class="form-control form-control-sm" placeholder="Address" data-bv-notempty="true" data-bv-notempty-message="The address is required and cannot be empty"></textarea>
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>Adhar No</label>
                                <input type="number" id="AdharNumber" required class="form-control form-control-sm" placeholder="Adhar No" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>PAN No</label>
                                <input type="text" id="PanNumber" required class="form-control form-control-sm" placeholder="PAN No" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>Contact Person</label>
                                <input type="text" id="ContactPerson" required class="form-control form-control-sm" placeholder="Contact Person" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>Contact No</label>
                                <input type="number" id="ContactNumber" required class="form-control form-control-sm" placeholder="Contact No" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>
                            <!--/. col -->
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>Email Address</label>
                                <input type="email" id="EmailAddress" required class="form-control form-control-sm" placeholder="Email Address" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>GST No</label>
                                <input type="text" id="GSTNumber" required class="form-control form-control-sm" placeholder="GST No" data-bv-notempty="true" data-bv-notempty-message="The LeadTime is required and cannot be empty">
                            </div>

                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3" style="display:none">
                                <label>Customer Type</label>
                                @Html.DropDownList("CustTypeID", new SelectList(ViewBag.CustType, "ID", "Type"), new { @class = "form-control form-control-sm" })

                            </div>
                            <div class="col-md-6 col-6 mb-3"  style="display:none">
                                <label>Customer ID</label>
                                <input type="text" id="CustID" class="form-control form-control-sm" placeholder="Customer ID">
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-6 mb-3">
                                <label>Sales Person</label>
                                @Html.DropDownList("SalesPersonID", new SelectList(ViewBag.SalesPerson, "ID", "Name"), new { @class = "form-control form-control-sm" })
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-12 mb-3">
                                <label>Adhar Upload</label>
                                <input type="file" id="AdharUpload" class="form-control form-control-sm" placeholder="Adhar Upload">
                            </div>
                            <!--/. col -->
                            <div class="col-md-6 col-12 mb-3">
                                <label>PAN Upload</label>
                                <input type="file" id="PanUpload" class="form-control form-control-sm" placeholder="PAN Upload">
                            </div>

                            <!--/. col -->
                            <div class="col-md-6 col-12 mb-3">
                                <label>Profile Photo</label>
                                <input type="file" id="ProfilePhoto" class="form-control form-control-sm" placeholder="" data-bv-notempty="true" data-bv-notempty-message="The Profile poto is required and cannot be empty">
                            </div>
                            <div class="col-md-6 col-12 mb-3">
                                <label>Username</label>
                                <input type="text" id="Username" required class="form-control form-control-sm" placeholder="Username" data-bv-notempty="true" data-bv-notempty-message="The Username is required and cannot be empty">
                            </div>
                            <div class="col-md-6 col-12 mb-3">
                                <label>Password</label>
                                <input type="password" id="Password" required class="form-control form-control-sm" placeholder="Password" data-bv-notempty="true" data-bv-notempty-message="The Password is required and cannot be empty">
                            </div>
                            <div class="col-md-6 col-12 mb-3">
                                <label>Confirm Password</label>
                                <input type="password" id="ConfirmPassword" required class="form-control form-control-sm" placeholder="Confirm Password" data-bv-notempty="true" data-bv-notempty-message="The Password is required and cannot be empty">
                            </div>
                            <!--/. row -->
                            <div class="col-12 text-center mb-3">
                                <button class="btn btn-primary btn-sm" id="addCustomer" type="button">Register</button>
                                @*<button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>*@
                            </div>


                        </div>
                        @Html.ActionLink("Already Register Login", "Login", "Account")
                    </div>
                </div>

            </div>
            <!--/. col-->
        </section>
    </div>




